{
  "manifest_version": 3,
  "name": "Random image extension",
  "action": {},
  "version": "1.0",
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "service_worker": "/scripts/service_worker.js"
  },
  "chrome_url_overrides" : {
    "newtab": "my_new_tab.html"
  }
}